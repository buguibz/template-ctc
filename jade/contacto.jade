<!DOCTYPE html>
html(lang="es")
head
  meta(charset="UTF-8")
  meta(name="viewport", content="width=device-width, initial-scale=1.0")
  meta(http-equiv="X-UA-Compatible", content="ie=edge")
  link(rel="stylesheet", href="../css/estilos.css")
  link(href="https://fonts.googleapis.com/css?family=Russo+One", rel="stylesheet")
  title Document
body
  div
    header
      .contenido
        hgroup
          h1 CHAPE TU CODIGO
        nav
          ul
            li
              a(href="index.html") inicio
            li
              a(href="tutoriales.html") tutoriales
            li
              a(href="contacto.html") contacto
            li
              a(href="media.html") media
        section
          .textoPre
            hgroup
              h1 Contactanos
            form(action="")
              label(for="nombre") nombre:
              input(type="text" id="nombre" placeholder="escribe tu nombre" required)
              label(for="correo") correo:
              input(type="email" id="correo" placeholder="escribe tu correo" required)
              label(for="edad") edad:
              input(type="number" id="edad" min='0' max='100') 
              label(for="calificacion") calificacion:
              input(type="range" id="calificacion" min="0" max="10" value="0") 
              label(for="fecha") fecha:
              input(type="date" id="fecha")
              input(type="submit" value='Enviar datos' id='btnsubmit')
  footer
    canvas#micanvas     
  script(src="../js/jquery-3.1.1.min.js")
  script(src="../js/jquery-1.12.3.min.js") 
  script(src="../js/principal.js") 
